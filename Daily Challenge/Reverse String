https://leetcode.com/problems/reverse-string/


*****************************************SOLUTION***************************************************

class Solution {
public:
void reverseString(vector& s)
{
int size = s.size();
int j = size - 1;

   for(int i = 0; i < size/2; i++)
   {
       swap(s[i] , s[j]);
       j--;
   }
}
